{
  "name": "sails-react-webpack",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "ejs": "2.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "1.0.1",
    "grunt-contrib-jst": "1.0.0",
    "grunt-contrib-less": "1.3.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sails-linker": "~0.10.1",
    "grunt-sync": "0.5.2",
    "include-all": "^1.0.0",
    "rc": "1.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "sails": "~0.12.14",
    "sails-disk": "~0.10.9"
  },
  "scripts": {
    "start": "npm-run-all --parallel open:client dev",
    "start:debug": "npm-run-all --parallel open:client debug",
    "open:client": "webpack-dev-server --open",
    "build": "npm run build:prod",
    "build:dev": "webpack",
    "build:prod": "webpack --config assets/config/webpack.prod",
    "clean": "rimraf .tmp && mkdirp .tmp/public",
    "prod": "sails lift --prod",
    "dev": "sails lift --dev",
    "debug": "node --inspect app.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/eduardo/sails-react-webpack.git"
  },
  "author": "eduardo",
  "license": "",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}
